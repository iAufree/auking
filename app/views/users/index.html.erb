<%= provide(:title, "Users") %>
<div class="users-panel">
	  <ul class="users-info">
		<% @users.each do |user| %>
		  	<li>
		  		<%= link_to(user) do %>
		  				<%= image_tag(user.avatar_url(:thumb), class: "avatar avatar-thumb") %>
			  		<%= user.name %>
				<% end %>
		  	</li>
		<% end %>
	  </ul>
</div>